{
  "name": "3dp-frontend",
  "private": true,
  "sideEffects": false,
  "scripts": {
    "start": "remix-serve build",
    "build": "npm run build:css && remix build",
    "build:css": "tailwindcss -m -i ./styles/app.css -o app/styles/app.css",
    "dev": "concurrently \"npm run dev:css\" \"remix dev\"",
    "dev:css": "tailwindcss -w -i ./styles/app.css -o app/styles/app.css"
  },
  "dependencies": {
    "@apollo/client": "3.7.0",
    "@blueprintjs/core": "4.11.3",
    "@blueprintjs/popover2": "1.7.3",
    "@polkadot/api": "9.5.1",
    "@polkadot/keyring": "10.1.10",
    "@polkadot/react-identicon": "2.9.11",
    "@polkadot/ui-keyring": "2.9.11",
    "@react-three/drei": "9.34.3",
    "@react-three/fiber": "8.8.10",
    "@remix-run/dev": "1.7.2",
    "@remix-run/eslint-config": "1.7.2",
    "@remix-run/node": "1.7.2",
    "@remix-run/react": "1.7.2",
    "@remix-run/serve": "1.7.2",
    "@types/three": "0.144.0",
    "autoprefixer": "10.4.12",
    "concurrently": "7.4.0",
    "eslint": "8.25.0",
    "graphql": "16.6.0",
    "jotai": "1.8.5",
    "moment": "2.29.4",
    "postcss": "8.4.17",
    "react": "18.2.0",
    "react-dom": "18.2.0",
    "react-moment": "1.1.2",
    "react-timeago": "7.1.0",
    "remix-utils": "3.3.0",
    "tailwindcss": "3.1.8",
    "three": "0.145.0",
    "three-stdlib": "2.17.2",
    "typescript": "4.8.4"
  },
  "packageManager": "yarn@3.2.4",
  "engines": {
    "node": ">=16"
  }
}
