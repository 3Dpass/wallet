{
  "name": "3dp-frontend",
  "private": true,
  "sideEffects": false,
  "scripts": {
    "preinstall": "npx only-allow pnpm",
    "start": "remix-serve build",
    "build": "pnpm build:css && remix build",
    "build:css": "tailwindcss -m -i ./styles/app.css -o app/styles/app.css",
    "dev": "concurrently \"pnpm dev:css\" \"remix dev\"",
    "dev:css": "tailwindcss -w -i ./styles/app.css -o app/styles/app.css"
  },
  "dependencies": {
    "@apollo/client": "3.7.10",
    "@blueprintjs/core": "4.17.5",
    "@blueprintjs/popover2": "1.13.9",
    "@polkadot/api": "9.14.2",
    "@polkadot/keyring": "11.0.1",
    "@polkadot/react-identicon": "3.0.1",
    "@polkadot/ui-keyring": "3.0.1",
    "@polkadot/util": "11.0.1",
    "@polkadot/util-crypto": "11.0.1",
    "@react-three/drei": "9.57.0",
    "@react-three/fiber": "8.11.9",
    "@remix-run/dev": "1.14.0",
    "@remix-run/eslint-config": "1.14.0",
    "@remix-run/node": "1.14.0",
    "@remix-run/react": "1.14.0",
    "@remix-run/serve": "1.14.0",
    "@types/three": "0.149.0",
    "autoprefixer": "10.4.13",
    "concurrently": "7.6.0",
    "eslint": "8.35.0",
    "graphql": "16.6.0",
    "jotai": "2.0.3",
    "moment": "2.29.4",
    "postcss": "8.4.21",
    "react": "18.2.0",
    "react-dom": "18.2.0",
    "react-moment": "1.1.3",
    "react-timeago": "7.1.0",
    "remix-utils": "6.0.0",
    "tailwindcss": "3.2.7",
    "three": "0.150.1",
    "three-stdlib": "2.21.8",
    "typescript": "4.9.5"
  },
  "engines": {
    "node": ">=16"
  }
}
