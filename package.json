{
  "name": "3dp-frontend",
  "private": true,
  "sideEffects": false,
  "scripts": {
    "start": "remix-serve build",
    "build": "npm run build:css && remix build",
    "build:css": "tailwindcss -m -i ./styles/app.css -o app/styles/app.css",
    "dev": "concurrently \"npm run dev:css\" \"remix dev\"",
    "dev:css": "tailwindcss -w -i ./styles/app.css -o app/styles/app.css"
  },
  "dependencies": {
    "@apollo/client": "3.6.9",
    "@blueprintjs/core": "4.10.1",
    "@blueprintjs/popover2": "1.6.6",
    "@polkadot/api": "9.4.1",
    "@polkadot/keyring": "10.1.8",
    "@polkadot/react-identicon": "2.9.9",
    "@polkadot/ui-keyring": "2.9.9",
    "@react-three/drei": "9.32.0",
    "@react-three/fiber": "8.8.2",
    "@remix-run/dev": "1.7.2",
    "@remix-run/eslint-config": "1.7.2",
    "@remix-run/node": "1.7.2",
    "@remix-run/react": "1.7.2",
    "@remix-run/serve": "1.7.2",
    "autoprefixer": "10.4.12",
    "graphql": "16.6.0",
    "jotai": "1.8.4",
    "moment": "2.29.4",
    "postcss": "8.4.16",
    "react": "18.2.0",
    "react-dom": "18.2.0",
    "react-moment": "1.1.2",
    "react-timeago": "7.1.0",
    "remix-utils": "3.3.0",
    "tailwindcss": "3.1.8",
    "three": "0.144.0",
    "three-stdlib": "2.16.1"
  },
  "devDependencies": {
    "@types/three": "0.144.0",
    "concurrently": "7.4.0",
    "eslint": "8.23.1",
    "typescript": "4.8.3"
  },
  "packageManager": "yarn@3.2.3",
  "engines": {
    "node": ">=16"
  }
}
