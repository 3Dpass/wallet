{
  "name": "3dp-frontend",
  "private": true,
  "sideEffects": false,
  "scripts": {
    "start": "remix-serve build",
    "build": "npm run build:css && remix build",
    "build:css": "tailwindcss -m -i ./styles/app.css -o app/styles/app.css",
    "dev": "concurrently \"npm run dev:css\" \"remix dev\"",
    "dev:css": "tailwindcss -w -i ./styles/app.css -o app/styles/app.css"
  },
  "dependencies": {
    "@remix-run/eslint-config": "1.6.4",
    "@remix-run/node": "1.6.4",
    "@remix-run/react": "1.6.4",
    "@remix-run/serve": "1.6.4",
    "autoprefixer": "10.4.7",
    "jotai": "1.7.4",
    "react": "18.2.0",
    "react-dom": "18.2.0",
    "react-timeago": "7.1.0",
    "three": "0.142.0",
    "three-stdlib": "2.12.1"
  },
  "devDependencies": {
    "@blueprintjs/core": "4.6.0",
    "@blueprintjs/popover2": "1.4.2",
    "@polkadot/api": "8.12.2",
    "@polkadot/keyring": "10.0.2",
    "@polkadot/react-identicon": "2.8.2",
    "@polkadot/ui-keyring": "2.8.2",
    "@react-three/drei": "9.17.0",
    "@react-three/fiber": "8.0.27",
    "@remix-run/dev": "1.6.4",
    "concurrently": "7.2.2",
    "eslint": "8.19.0",
    "postcss": "8.4.14",
    "tailwindcss": "3.1.5",
    "typescript": "4.7.4"
  },
  "engines": {
    "node": ">=16"
  }
}
